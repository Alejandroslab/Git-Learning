git commit -m "Update"

or PowerShell:

git add . ; git commit -m "Update" ; git push


for i in *; do echo && cd $i && pwd && echo "Fetching.." && git fetch && echo "Pushing now.." && git add . && git commit -m "Update" && git push && cd .. ; done


for i in *; do cd $i && pwd && git fetch && cd ..; done


for i in *; do echo && cd $i && pwd && git fetch && git add . && if git commit -m "Update" | grep -q "nothing to commit" ; then echo "There is nothing to push. Your branch is already up to date."; else echo "Ok. Pushing" && git commit -m "Update $(date +%Y%m%d)" && git push ; fi; cd ..; done
